style	chrome://calendar/content/calendar-event-dialog.xul			chrome://MonterailOverlay/skin/css/lightning.css

#register theme
skin	MonterailOverlay	classic/1.0	chrome/MonterailOverlay/classic/
skin	MonterailOverlay	MonterailBlue	chrome/MonterailOverlay/
skin	app_version		MonterailBlue	chrome/MonterailOverlay/52/
skin	app_version		MonterailBlue	chrome/MonterailOverlay/60/	appversion>=60a1
skin	os_special		MonterailBlue	chrome/MonterailOverlay/linux/
skin	os_special		MonterailBlue	chrome/MonterailOverlay/mac/	os=Darwin
skin	os_special		MonterailBlue	chrome/MonterailOverlay/win/	os=WINNT

